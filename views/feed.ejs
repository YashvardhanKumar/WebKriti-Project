<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/nav2.css">
  <link rel="stylesheet" href="/feedStyle.css">
  <title>DeYaKu</title>
</head>
<style>
</style>

<body>
  <%- include('partial/feedNav.ejs') %>

    <!-- <div class="suggested">
        ccccccc
      </div> -->
    <div class="postsList">
      <% for( let i=0; i < 10; i++ ) { %>

        <div class="post">
          <div class="poster">
            <div class="posterPfp">
              <img src="/yash.png" width="100%" height="100%" alt="">
            </div>
            <div class="posterName">
              <div class="userId">
                <a href="/<%= id %>/profile ">
                  unstableguy
                </a>
              </div>
              <div class="username">
                Yashvardhan Kumar
              </div>
            </div>
            <div class="postOptions" style="float: right;">
              <svg class="icon" xmlns="http://www.w3.org/2000/svg" height="4vmin" viewBox="0 0 24 24" width="4vmin"
                fill="#ffffff91">
                <path d="M0 0h24v24H0z" fill="none" />
                <path d="M3 18h18v-2H3v2zm0-5h18v-2H3v2zm0-7v2h18V6H3z" />
              </svg>
              <div class="dropdown-content">
                <form  class="content-1" action="/<%=id%>/postId/<%=id%>/edit" method="post">
                  <button>Edit</button>
                </form>
                <form  class="content-2" action="/<%=id%>/postId/<%=id%>/delete" method="post">
                  <button>Delete</button>
                </form>
                <form  class="content-3" action="/<%=id%>/postId/archive<%=id%>/edit" method="post">
                  <button>Archive</button>
                </form>
              </div>
            </div>
          </div>
          <div class="postContent">
            <div class="postImage">
              <img src="/mountains.png"  width="100%" alt="">
            </div>
            <div class="postCaption">
              <span>hello</span>
            </div>
          </div>
          <div class="react">
            <div class="like" id='clicklike'>
              <form action="/<%=id%>/postId<%=id%>" method="post">

                <button name="liked" id="likebtn" value="<% if (id == 4) { %>1<% } else { %>0<% } %>" ondblclick="unlike()" class="likebtn">
                  <svg id="like" xmlns="http://www.w3.org/2000/svg" height="4vmin" viewBox="0 0 24 24" width="4vmin" fill="<% if (id == 4) { %>red<% } else { %>#ffffff91<% } %>">
                    <path d="M0 0h24v24H0V0z" fill="none" />
                    <path
                    d="M16.5 5c-1.54 0-3.04.99-3.56 2.36h-1.87C10.54 5.99 9.04 5 7.5 5 5.5 5 4 6.5 4 8.5c0 2.89 3.14 5.74 7.9 10.05l.1.1.1-.1C16.86 14.24 20 11.39 20 8.5c0-2-1.5-3.5-3.5-3.5z"
                    opacity="<% if (id == 4) { %>1<% } else { %>0.3<% } %>" />
                    <path
                    d="M16.5 3c-1.74 0-3.41.81-4.5 2.09C10.91 3.81 9.24 3 7.5 3 4.42 3 2 5.42 2 8.5c0 3.78 3.4 6.86 8.55 11.54L12 21.35l1.45-1.32C18.6 15.36 22 12.28 22 8.5 22 5.42 19.58 3 16.5 3zm-4.4 15.55l-.1.1-.1-.1C7.14 14.24 4 11.39 4 8.5 4 6.5 5.5 5 7.5 5c1.54 0 3.04.99 3.57 2.36h1.87C13.46 5.99 14.96 5 16.5 5c2 0 3.5 1.5 3.5 3.5 0 2.89-3.14 5.74-7.9 10.05z" />
                  </svg>
                </button>
              </form>
            </div>
            <div class="showComments">
              <svg class="icon" xmlns="http://www.w3.org/2000/svg" height="4vmin" viewBox="0 0 24 24" width="4vmin" fill="#ffffff91">
                <path d="M0 0h24v24H0V0z" fill="none" />
                <path d="M21.99 4c0-1.1-.89-2-1.99-2H4c-1.1 0-2 .9-2 2v12c0 1.1.9 2 2 2h14l4 4-.01-18zM17 14H7c-.55 0-1-.45-1-1s.45-1 1-1h10c.55 0 1 .45 1 1s-.45 1-1 1zm0-3H7c-.55 0-1-.45-1-1s.45-1 1-1h10c.55 0 1 .45 1 1s-.45 1-1 1zm0-3H7c-.55 0-1-.45-1-1s.45-1 1-1h10c.55 0 1 .45 1 1s-.45 1-1 1z" />
              </svg>
            </div>
            <div class="share">
              <svg class="icon" xmlns="http://www.w3.org/2000/svg" height="4vmin" viewBox="0 0 24 24" width="4vmin" fill="#ffffff91">
                <path d="M0 0h24v24H0V0z" fill="none" />
                <path d="M18 16.08c-.76 0-1.44.3-1.96.77L8.91 12.7c.05-.23.09-.46.09-.7s-.04-.47-.09-.7l7.05-4.11c.54.5 1.25.81 2.04.81 1.66 0 3-1.34 3-3s-1.34-3-3-3-3 1.34-3 3c0 .24.04.47.09.7L8.04 9.81C7.5 9.31 6.79 9 6 9c-1.66 0-3 1.34-3 3s1.34 3 3 3c.79 0 1.5-.31 2.04-.81l7.12 4.16c-.05.21-.08.43-.08.65 0 1.61 1.31 2.92 2.92 2.92s2.92-1.31 2.92-2.92-1.31-2.92-2.92-2.92z" />
              </svg>
            </div>
            <div class="writeComment">

              <svg class="icon" xmlns="http://www.w3.org/2000/svg" height="4vmin" viewBox="0 0 24 24" width="4vmin" fill="#ffffff91">
                <path d="M0 0h24v24H0V0z" fill="none" />
                <path
                d="M22 4c0-1.1-.9-2-2-2H4c-1.1 0-2 .9-2 2v12c0 1.1.9 2 2 2h14l4 4V4zm-6 7h-3v3c0 .55-.45 1-1 1s-1-.45-1-1v-3H8c-.55 0-1-.45-1-1s.45-1 1-1h3V6c0-.55.45-1 1-1s1 .45 1 1v3h3c.55 0 1 .45 1 1s-.45 1-1 1z" />
              </svg>
              <div>
                <form action="/<%=id%>/postId<%=id%>" method="post">
                  <textarea style="resize: none;" rows="2" placeholder="Comment" name="comment" id="comment"></textarea>
                  <button id="postcomment">
                  <svg class="icon" xmlns="http://www.w3.org/2000/svg" height="4vmin" viewBox="0 0 24 24" width="4vmin" fill="#ffffff91">
                    <path d="M0 0h24v24H0z" fill="none" />
                    <path d="M2.01 21L23 12 2.01 3 2 10l15 2-15 2z" />
                  </svg>
                  </button>
                </form>
              </div>
            </div>
            <div class="saveToProfile">
              <svg class="icon" xmlns="http://www.w3.org/2000/svg" height="4vmin" viewBox="0 0 24 24" width="4vmin" fill="#ffffff91">
                <path d="M0 0h24v24H0V0z" fill="none" />
                <path d="M19 18l2 1V3c0-1.1-.9-2-2-2H8.99C7.89 1 7 1.9 7 3h10c1.1 0 2 .9 2 2v13zM15 5H5c-1.1 0-2 .9-2 2v16l7-3 7 3V7c0-1.1-.9-2-2-2z" />
              </svg>
            </div>
          </div>
        </div>
        <% } %>
    </div>
</body>
<script>

</script>

</html>